<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<servlet>
		<servlet-name>Index</servlet-name>
		<servlet-class>ar.edu.itba.paw.g4.controller.IndexController
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MovieList</servlet-name>
		<servlet-class>ar.edu.itba.paw.g4.controller.MovieListController
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>ar.edu.itba.paw.g4.controller.LoginController
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Registration</servlet-name>
		<servlet-class>ar.edu.itba.paw.g4.controller.RegistrationController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MovieList</servlet-name>
		<url-pattern>/movieList</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Registration</servlet-name>
		<url-pattern>/registration</url-pattern>
	</servlet-mapping>
</web-app>